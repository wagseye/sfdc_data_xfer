<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Path_begins_with_slash</fullName>
    <active>true</active>
    <errorConditionFormula>!BEGINS( Path__c ,&quot;/&quot;)</errorConditionFormula>
    <errorMessage>The path field must begin with a slash character</errorMessage>
</ValidationRule>
